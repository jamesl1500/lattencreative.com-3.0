{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "tagline": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "icon": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "pricing": {
      "type": "component",
      "component": "pricing.pricing",
      "repeatable": true
    },
    "processSteps": {
      "type": "component",
      "component": "process.process-step",
      "repeatable": true
    },
    "testimonials": {
      "type": "component",
      "component": "testimonial.testimonial",
      "repeatable": true
    },
    "features": {
      "type": "text"
    }
  }
}
