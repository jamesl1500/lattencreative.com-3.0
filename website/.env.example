# Development Environment Configuration
# This file contains safe defaults for development
# For production, copy .env.example and fill with real values

# Strapi API URL
NEXT_PUBLIC_STRAPI_API_URL=http://127.0.0.1:1337/api

# Strapi URL
NEXT_PUBLIC_STRAPI_URL=http://127.0.0.1:1337

# Strapi API authentication token
# NOTE: Use a development token here, keep production tokens secure
STRAPI_API_TOKEN=your-development-token-here

# Next.js site URL
NEXT_PUBLIC_SITE_URL=http://localhost:3000/

# Next.js site environment
NEXT_PUBLIC_ENV=development

# Enable debug mode
NEXT_PUBLIC_DEBUG=true

# Google Analytics ID (development placeholder)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

NEXT_PUBLIC_RESEND_API_KEY=api-key-here